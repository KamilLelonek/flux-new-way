    "classnames": "^1.2.0",
    "express": "^4.12.3",
    "file-loader": "^0.8.1",
    "imports-loader": "^0.6.3",
    "jquery": "^2.1.3",
    "react": "^0.13.1",
    "react-router": "^0.13.1",
    "react-router-bootstrap": "^0.12.1",
    "sugar": "^1.4.1",
